<config>
    <native
        xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
        <interface>
            <GigabitEthernet>
                <name>1</name>
                <ip>
                    <address>
                        <dhcp/>
                    </address>
                    <igmp
                        xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-igmp">
                        <explicit-tracking>false</explicit-tracking>
                        <proxy-service>false</proxy-service>
                        <unidirectional-link>false</unidirectional-link>
                        <v3lite>false</v3lite>
                    </igmp>
                    <nat
                        xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-nat">
                        <outside/>
                    </nat>
                </ip>
                <mop>
                    <enabled>false</enabled>
                    <sysid>false</sysid>
                </mop>
                <crypto
                    xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-crypto">
                    <map>
                        <tag>MYMAP</tag>
                    </map>
                </crypto>
            </GigabitEthernet>
        </interface>
        <crypto>
            <ipsec
                xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-crypto">
                <transform-set>
                    <tag>MYTRANS</tag>
                    <esp>esp-aes</esp>
                    <key-bit>256</key-bit>
                    <esp-hmac>esp-sha-hmac</esp-hmac>
                    <mode>
                        <tunnel/>
                    </mode>
                </transform-set>
            </ipsec>
            <isakmp
                xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-crypto">
                <key>
                    <key-address>
                        <key>VAULT</key>
                        <addr4-container>
                            <address>{{PEER_IPADD}}</address>
                        </addr4-container>
                    </key-address>
                </key>
                <policy>
                    <number>10</number>
                    <authentication>pre-share</authentication>
                    <encryption>
                        <aes>
                            <key>256</key>
                        </aes>
                    </encryption>
                    <group>5</group>
                </policy>
            </isakmp>
            <map
                xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-crypto">
                <map-seq>
                    <map>
                        <name>MYMAP</name>
                        <seq>10</seq>
                        <ipsec-isakmp/>
                        <match>
                            <address>100</address>
                        </match>
                        <set>
                            <peer>
                                <address>{{PEER_IPADD}}</address>
                            </peer>
                            <pfs>
                                <group>group5</group>
                            </pfs>
                            <transform-set>MYTRANS</transform-set>
                            <security-association>
                                <lifetime>
                                    <seconds>1800</seconds>
                                </lifetime>
                            </security-association>
                        </set>
                    </map>
                </map-seq>
            </map>
        </crypto>
    </native>
</config>


